// enter programpaper 90set a 15set x 0set y 0set space 3set dspace 2set ratio 3repeat x1 0 a{   repeat y1 0 a   {      set x2 (x1 + x)      set y2 (y1 + y)      set color (x2 + y2)      set x3 (x2 * space)      set y3 (y2 * space)      set x4 (x3 * ratio-dspace)      set y4 (y3 * ratio-dspace)      pen (100 - color)      line x3 y3 x4 y4      set [x3 y3] (color + 40)      set [x4 y4] (color + 40)   }}