paper 0forever{   set mx <mouse 1>   set my <mouse 2>   set mb <mouse 3>   paper mb   pen (100-mb)   line (mx-11) (my-11) 0 0   line (mx-11) (my+11) 0 100   line (mx+11) (my-11) 100 0   line (mx+11) (my+11) 100 100      line (mx-11) (my-11) (mx-11) (my+11)   line (mx-11) (my-11) (mx+11) (my-11)   line (mx+11) (my+11) (mx+11) (my-11)   line (mx-11) (my+11) (mx+11) (my+11)}