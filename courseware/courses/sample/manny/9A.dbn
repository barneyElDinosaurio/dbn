// enter programpaper 0forever{   set nx <net 1>   set ny <net 2>   repeat a 0 100   {      pen (a-nx)      line a 0 a 100      pen (a-ny)      line 0 a 100 a      pen (nx)      line (100-a) 0 (100-a) 100      pen (ny)      line 0 (100-a) 100 (100-a)   }   set <net 3> nx   set <net 4> ny   set n5x <net 5>   set n6x <net 6>   pen 20   line (n5x-11) (n6x-11) (n5x-11) (n6x+11)   line (n5x-11) (n6x-11) (n5x+11) (n6x-11)   line (n5x+11) (n6x+11) (n5x+11) (n6x-11)   line (n5x-11) (n6x+11) (n5x+11) (n6x+11)   set n7x <net 7>   set n8y <net 8>   pen 80   line (n7x-11) (n8y-11) 0 0   line (n7x-11) (n8y+11) 0 100   line (n7x+11) (n8y-11) 100 0   line (n7x+11) (n8y+11) 100 100}