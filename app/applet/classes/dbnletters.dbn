// dbnletters
command typea h v
{
  line h v h (v+7)
  line (h) (v+7) (h+3) (v+10)
  line (h+3) (v+10) (h+10) (v+3)
  line (h+10) (v+3) (h+10) v
  line h (v+3) (h+10) (v+3)
}
command typeb h v
{
  line h v h (v+10)
  line h (v+10) (h+5) (v+10)
  line (h+5) (v+10) (h+8) (v+7)
  line h (v+6) (h+7) (v+6)
  line (h+7) (v+6) (h+10) (v+3)
  line (h+10) (v+3) (h+10) (v+1)
  line h v (h+9) v
}
command typec h v
{
  line (h+4) v (h+10) v
  line (h+4) v h (v+4)
  line h (v+4) h (v+9)
  line (h+1) (v+10) (h+9) (v+10)
}
command typed h v
{
  line h v h (v+10)
  line h v (h+8) v
  line (h+8) v (h+10) (v+2)
  line (h+10) (v+2) (h+10) (v+6)
  line (h+10) (v+6) (h+6) (v+10)
  line (h+6) (v+10) h (v+10)
}
command typee h v
{
  line h (v+3) h (v+10)
  line h (v+3) (h+3) v
  line (h+3) v (h+10) v
  line h (v+6) (h+9) (v+6)
  line h (v+10) (h+9) (v+10)
}
command typef h v
{
  line h v h (v+10)
  line h (v+6) (h+8) (v+6)
  line h (v+10) (h+10) (v+10)
}
command typeg h v
{
  line (h+4) v (h+9) v
  line (h+4) v h (v+4)
  line h (v+4) h (v+9)
  line (h+1) (v+10) (h+9) (v+10)
  line (h+10) (v+1) (h+10) (v+5)
  line (h+10) (v+5) (h+6) (v+5)
}
command typeh h v
{
  line h v h (v+10)
  line h (v+4) (h+10) (v+4)
  line (h+10) v (h+10) (v+10)
}
command typei h v
{
  line h v (h+10) v
  line (h+5) v (h+5) (v+10)
  line h (v+10) (h+9) (v+10)
}
command typej h v
{
  line h (v+3) (h+3) v
  line (h+3) v (h+9) v
  line (h+10) (v+1) (h+10) (v+10)
}
command typek h v
{
  line h v h (v+10)
  line h (v+1) (h+9) (v+10)
  line (h+5) (v+5) (h+10) v
}
command typel h v
{
  line h v h (v+10)
  line h v (h+10) v
}
command typem h v
{
  line h v h (v+10)
  //line h (v+10) (h+2) (v+10)
  line (h+1) (v+10) (h+5) (v+6)
  line (h+5) (v+6) (h+9) (v+10)
  line (h+10) (v+10) (h+10) v
}
command typen h v
{
  line h v h (v+10)
  line h (v+10) (h+3) (v+10)
  line (h+3) (v+10) (h+10) (v+3)
  line (h+10) (v+10) (h+10) v
}
command typeo h v
{
  line (h+4) v (h+9) v
  line (h+4) v h (v+4)
  line h (v+4) h (v+9)
  line (h+1) (v+10) (h+7) (v+10)
  line (h+7) (v+10) (h+10) (v+7)
  line (h+10) (v+7) (h+10) (v+1)
}
command typep h v
{
  line h v h (v+10)
  line h (v+10) (h+7) (v+10)
  line (h+7) (v+10) (h+10) (v+7)
  line (h+10) (v+6) (h+8) (v+4)
  line h (v+4) (h+8) (v+4)
}
command typeq h v
{
  line (h+4) v (h+8) v
  line (h+4) v h (v+4)
  line h (v+4) h (v+9)
  line (h+1) (v+10) (h+7) (v+10)
  line (h+7) (v+10) (h+10) (v+7)
  line (h+10) (v+7) (h+10) (v+2)
  line (h+6) (v+4) (h+10) v
}
command typer h v
{
  line h v h (v+10)
  line h (v+10) (h+7) (v+10)
  line (h+7) (v+10) (h+10) (v+7)
  line (h+10) (v+6) (h+8) (v+4)
  line h (v+4) (h+8) (v+4)
  line (h+6) (v+4) (h+10) v
}
command types h v
{
  line h (v+2) (h+2) v
  line (h+2) v (h+9) v
  line (h+10) (v+1) (h+10) (v+4)
  line (h+9) (v+5) (h+2) (v+5)
  line (h+2) (v+5) h (v+7)
  line h (v+7) h (v+9)
  line (h+1) (v+10) (h+9) (v+10)
  line (h+9) (v+10) (h+10) (v+9)
}
command typet h v
{
  line h (v+10) (h+10) (v+10)
  line (h+5) (v+10) (h+5) v
}
command typeu h v
{
  line h (v+10) h (v+3)
  line h (v+3) (h+3) v
  line (h+3) v (h+9) v
  line (h+10) (v+1) (h+10) (v+10)
}
command typev h v
{
  line h (v+10) h (v+5)
  line h (v+5) (h+5) v
  line (h+5) v (h+10) (v+5)
  line (h+10) (v+5) (h+10) (v+10)
}
command typew h v
{
  line h (v+10) h (v+3)
  line h (v+3) (h+3) v
  line (h+3) v (h+6) (v+3)
  line (h+6) (v+3) (h+9) v
  line (h+10) (v+1) (h+10) (v+10)
}
command typex h v
{
  line h (v+10) h (v+9)
  line h (v+9) (h+4) (v+5)
  line (h+4) (v+5) (h+6) (v+5)
  line (h+6) (v+5) (h+10) (v+9)
  line (h+10) (v+9) (h+10) (v+10)
  line h v h (v+1)
  line h (v+1) (h+4) (v+5)
  line (h+6) (v+5) (h+10) (v+1)
  line (h+10) (v+1) (h+10) v
}
command typex2 h v
{
  line h v (h+10) (v+10)
  line h (v+10) (h+10) v
}
command typey h v
{
  line h (v+10) h (v+7)
  line h (v+7) (h+3) (v+4)
  line (h+3) (v+4) (h+10) (v+4)
  line (h+10) (v+10) (h+10) (v+1)
  line (h+9) v (h+2) v
  line (h+2) v h (v+2)
}
command typez h v
{
  line h (v+10) (h+10) (v+10)
  line (h+10) (v+10) h v
  line h v (h+10) v
}
command letter l h v
{
  same? l 1
  {
    typea h v
  }
  same? l 2
  {
    typeb h v
  }
  same? l 3
  {
    typec h v
  }
  same? l 4
  {
    typed h v
  }
  same? l 5
  {
    typee h v
  }
  same? l 6
  {
    typef h v
  }
  same? l 7
  {
    typeg h v
  }
  same? l 8
  {
    typeh h v
  }
  same? l 9
  {
    typei h v
  }
  same? l 10
  {
    typej h v
  }
  same? l 11
  {
    typek h v
  }
  same? l 12
  {
    typel h v
  }
  same? l 13
  {
    typem h v
  }
  same? l 14
  {
    typen h v
  }
  same? l 15
  {
    typeo h v
  }
  same? l 16
  {
    typep h v
  }
  same? l 17
  {
    typeq h v
  }
  same? l 18
  {
    typer h v
  }
  same? l 19
  {
    types h v
  }
  same? l 20
  {
    typet h v
  }
  same? l 21
  {
    typeu h v
  }
  same? l 22
  {
    typev h v
  }
  same? l 23
  {
    typew h v
  }
  same? l 24
  {
    typex h v
  }
  same? l 25
  {
    typey h v
  }
  same? l 26
  {
    typez h v
  }
}
command testdbnletters
{
paper 0
pen 100
set x 17
set y 73
set dx 12
set dy 12
set n 0
repeat a 1 25
{
  letter a x y
  set x (x+dx)
  set n (n+1)
  same? n 5
  {
    set x 17
    set y (y-dy)
    set n 0
  }
}
set x (x+dx*5)
letter 26 x y
}

